{
  "expo": {
    "name": "read",
    "slug": "read",
    "scheme": "reader-app",
    "description": "Xano Authentication Starter App - includes Login, Registration, and Home screens",
    "privacy": "unlisted",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/app/icon.png",
    "splash": {
      "image": "./assets/app/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#1B1B40"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "io.bhanu.read",
      "supportsTablet": null,
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSCameraUsageDescription": "$(PRODUCT_NAME) needs access to your Camera.",
        "NSMicrophoneUsageDescription": "$(PRODUCT_NAME) needs access to your Microphone."
      },
      "usesIcloudStorage": false
    },
    "android": {
      "versionCode": 1,
      "package": "io.bhanu.read",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "display": null,
      "barStyle": null,
      "themeColor": "#000000",
      "orientation": null,
      "backgroundColor": null,
      "preferRelatedApplications": null
    },
    "notification": {
      "androidMode": "default"
    },
    "plugins": [],
    "extra": {
      "eas": {
        "projectId": "cbf1ec88-2ad1-4578-b198-edd2543c7dd8"
      }
    }
  }
}
